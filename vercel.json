{
    "builds": [
      {
        "src": "medicare/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12"
        },
        "env": {
          "PYTHONUSERBASE": "./python_modules"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "medicare/wsgi.py"
      }
    ]
  }
  